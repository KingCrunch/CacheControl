{
  "name": "crunch/cache-control",
  "description": "Interact with apcu/opcache in FPM-instances",
  "type": "library",
  "keywords": [
    "opcache",
    "apcu"
  ],
  "license": "MIT",
  "prefer-stable": true,
  "authors": [
    {
      "name": "Sebastian Krebs",
      "email": "sebastian@krebs.one",
      "homepage": "http://www.kingcrunch.de",
      "role": "Maintainer"
    }
  ],
  "require": {
    "php": "~7.0",
    "symfony/console": "~2.3",
    "crunch/fastcgi": "~1.0"
  },
  "require-dev": {
    "phake/phake": "~2.0@dev"
  },
  "support": {
    "email": "krebs.seb@gmail.com",
    "issues": "https://github.com/KingCrunch/CacheControl/issues"
  },
  "autoload": {
    "psr-4": {
      "Crunch\\CacheControl\\": "src"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0-dev"
    }
  },
  "bin": [
    "bin/cache-control",
    "bin/cache-control.php"
  ]
}
